function draw_a_square_20() {
  penWidth(1);
  for (var count = 0; count < 4; count++) {
    moveForward(20);
    turnLeft(90);
  }
}

function draw_a_line_of_squares() {
  for (var count2 = 0; count2 < 12; count2++) {
    draw_a_square_20();
    jumpForward(20);
  }
}

for (var count3 = 0; count3 < 5; count3++) {
  draw_a_line_of_squares();
  moveForward(60);
  turnLeft(144);
}
